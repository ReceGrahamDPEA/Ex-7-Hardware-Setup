<MainScreen>:
    #: import time __main__.time

    name: 'main'
    FloatLayout:
        size_hint: None, None

        DPEAButton:
            id: btn
            size: 100, 100
            x: root.width * .5
            y: root.height * .5
            text: "Off" if btn.state == "normal" else "On"
            color: .8, .8, .5, 1
            